{
  "name": "deepFocus",
  "version": "1.2.0",
  "description": "DeepFocus is a productivity tool that helps you stay focused and productive by tracking your time spent on your computer.",
  "main": "./out/main/main.js",
  "author": "Timeo Williams",
  "homepage": "https://deepFocus.cc",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "test:e2e": "npx playwright test",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -- --inspect ",
    "build": "NODE_ENV=production npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps && npx electron-rebuild",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "NODE_ENV=production electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@deepfocus/get-windows": "^1.0.19",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@kobalte/core": "^0.13.6",
    "@modular-forms/solid": "^0.23.0",
    "@solidjs/router": "^0.14.7",
    "chart.js": "^4.4.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.4",
    "gsap": "^3.12.5",
    "node-addon-api": "^8.2.0",
    "node-mac-permissions": "^2.4.0",
    "node-schedule": "^2.1.1",
    "simple-plist": "^1.3.1",
    "solid-chartjs": "^1.3.11",
    "solid-js": "^1.9.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@playwright/test": "^1.47.2",
    "@types/node": "^22.7.4",
    "autoprefixer": "^10.4.20",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "electron-playwright-helpers": "^1.7.1",
    "electron-vite": "^2.3.0",
    "eslint": "^9.11.1",
    "eslint-plugin-solid": "^0.14.3",
    "playwright": "^1.47.2",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.3",
    "semantic-release": "^24.1.2",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "valibot": "^0.42.1",
    "vinxi": "^0.4.3",
    "vite": "^5.4.8",
    "vite-plugin-solid": "^2.10.2"
  },
  "pnpm": {
    "resolutions": {
      "eslint": "^9.11.1"
    }
  }
}
